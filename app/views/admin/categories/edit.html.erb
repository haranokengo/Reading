<div class="container">
  <div class="row">
    <div class="col-md-6 offset-3 text-center">
      <%= form_with model: @category, url:admin_category_path, local: true do |f| %>
        <h4>カテゴリー名</h4>
        <div class="form-group">
          <%= f.text_field :name %>
        </div>
        <%= f.submit '保存' %>
      <% end %>
    </div>
  </div>
</div>